conda deactivate

sudo apt install -y build-essential libffi-dev git libssl-dev zlib1g-dev liblzma-dev libbz2-dev libreadline-dev libsqlite3-dev
curl https://pyenv.run | bash

export PATH="$HOME/.pyenv/bin:$PATH"
eval "$(pyenv init --path)"
eval "$(pyenv virtualenv-init -)"

source ~/.bashrc

pyenv install 3.10
pyenv local 3.10

python3.10 -m venv .venv
source .venv/bin/activate

pip install jupyter ipykernel

